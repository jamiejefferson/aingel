{
  "name": "aingel",
  "version": "1.0.0",
  "description": "Local LLM coding assistant that connects to LM Studio",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "aingel": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "chmod +x dist/index.js",
    "prepare": "npm run build",
